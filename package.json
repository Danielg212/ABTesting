{
  "name": "abtest-service",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run server:build | (npm run build:wait && nodemon dist/index.js)",
    "dev": "nodemon src/index.ts",
    "test": "jest  --no-cache",
    "build": "npm run server:build",
    "server:build": "rm -rf dist/*  && tsc -p src",
    "build:wait": "sleep 1 && until [ -f ./dist/index.js ]; do sleep 1; done;",
    "lint": "npm run eslint:base && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:write",
    "eslint:base": "eslint \"src/**/*.ts\"",
    "eslint:fix": "npm run eslint:base -- --fix",
    "prettier:base": "prettier \"src/**/*.ts\"",
    "prettier:check": "npm run prettier:base -- --check",
    "prettier:write": "npm run prettier:base -- --write"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/morgan": "^1.9.9",
    "cookie-parser": "^1.4.6",
    "express": "^4.19.2",
    "morgan": "^1.10.0",
    "tslog": "^4.9.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.3",
    "prettier": "^3.3.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
